extends layouts/main-layout.pug
 
block content
    main
        if prods.length > 0
            .grid
                each product in prods
                    article.card.product-item
                        header.card__header
                            h1.product__title #{product.title}
                        div.card__image
                            img(src="data:image/jpeg;base64,", alt="book1")
                        div.card__content
                            h2.product__price $19.99
                            p.product__description test
                        .card__action
                            button.btn Add to Cart
        else
            h1 No Products